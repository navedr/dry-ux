{
  "name": "dry-ux",
  "version": "1.55.0",
  "description": "",
  "main": "dist/index",
  "scripts": {
    "copy-css": "copyfiles -u 2 \"./src/enhanced-inputs/styles.css\" \"./dist/enhanced-inputs\"",
    "build": "rimraf dist/ && tsc && npm run copy-css",
    "watch": "rimraf dist/ && tsc --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "update:version": "npm version minor --no-git-tag-version",
    "prepub:npm": "npm run format && npm run build && npm run update:version",
    "publish:npm": "npm run prepub:npm && npm publish",
    "pack": "npm run build && npm pack --pack-destination pack/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navedr/dry-ux.git"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [],
  "author": "Naved Rangwala",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "@types/react": "^17.0.0",
    "@types/react-bootstrap": "^0.32.25",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "*"
  },
  "dependencies": {
    "react-bootstrap": "^0.33.1"
  },
  "peerDependencies": {
    "@types/react": ">=16.14.8",
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "bugs": {
    "url": "https://github.com/navedr/dry-ux/issues"
  },
  "homepage": "https://github.com/navedr/dry-ux#readme"
}
